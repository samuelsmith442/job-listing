# Enable legacy peer deps to avoid peer dependency issues
legacy-peer-deps = true

# Set the package manager to yarn
engine-strict = true

# Set the registry to the public npm registry
registry=https://registry.npmjs.org/

# Enable package-lock.json
package-lock = true

# Set the cache settings
cache = ~/.npm
cache-min = 10
cache-max = 999999999

# Set the log level
loglevel = error

# Set the progress bar
progress = true

# Set the save exact version
save-exact = true

# Set the save prefix
save-prefix = ^

# Set the engine strict
engine-strict = true

# Set the fund
fund = false

# Set the audit
audit = false

# Set the prefer-offline
prefer-offline = true

# Set the fetch-retries
fetch-retries = 5

# Set the fetch-retry-mintimeout
fetch-retry-mintimeout = 10000

# Set the fetch-retry-maxtimeout
fetch-retry-maxtimeout = 60000

# Set the fetch-timeout
fetch-timeout = 300000

# Set the fetch-retry-factor
fetch-retry-factor = 10

